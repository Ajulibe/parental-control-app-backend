  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./app.ts | ./node_modules/.bin/bunyan",
    "pm:start": "npm run build && pm2 start ecosystem.config.js -i 10 --attach --watch | ./node_modules/.bin/bunyan",
    "stop": "pm2 stop all",
    "build:watch": "npm run build -- -W",
    "monitor": "pm2 monit",
    "delete": "pm2 delete all",
    "dev": "nodemon -r tsconfig-paths/register ./app.ts | ./node_modules/.bin/bunyan",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "ttsc -p .",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:setup": "chmod +x ./init_db.sh && ./init_db.sh",
    "deploy": "serverless deploy",
    "offline": "serverless offline"
